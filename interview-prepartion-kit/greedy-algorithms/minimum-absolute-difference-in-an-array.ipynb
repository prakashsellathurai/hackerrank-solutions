    "def minimumAbsoluteDifference(arr):\n",
    "    arr.sort()\n",
    "    n = len(arr)\n",
    "    mini = max(arr)\n",
    "    for i in range(n - 1):\n",
    "        mini = min(mini, abs(arr[i] - arr[i+1]))\n",
    "    return mini\n",
    "\n",
    "arr = list(map(int, \"1 -3 71 68 17\".rstrip().split()))\n",
    "\n",
    "print(minimumAbsoluteDifference(arr))"
   ]
  }
