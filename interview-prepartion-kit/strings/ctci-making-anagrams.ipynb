    "# Complete the makeAnagram function below.\n",
    "def makeAnagram(a, b):\n",
    "    buffer = [0] * 26\n",
    "    for char in a: \n",
    "        buffer[ord(char) - ord('a')] += 1\n",
    "    for char in b: \n",
    "        buffer[ord(char) - ord('a')] -= 1\n",
    "    return sum(map(abs, buffer)) \n",
    "\n",
    "\n",
    "\n",
    "\n",
    "a=\"cde\"\n",
    "b=\"abc\"\n",
    "makeAnagram(a, b)"
   ]
